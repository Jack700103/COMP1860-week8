CHIP DivideByFour {
    IN Z;
    OUT out[2];
    
    PARTS:
    DFF(in=d0, out=q0);
    DFF(in=d1, out=q1);
    
    Or(a=q0, b=false, out=out[0]);
    Or(a=q1, b=false, out=out[1]);

    Not(in=q0, out=notq0);

    Xor(a=q1, b=q0, out=xorq1q0);
    Not(in=xorq1q0, out=xnorq1q0);

    Mux(a=xorq1q0, b=xnorq1q0, sel=Z, out=d1);

    Or(a=notq0, b=false, out=d0);
}